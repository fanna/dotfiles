		"terminal.ansiMagenta": "#af8afe",
		"terminal.ansiCyan": "#718488",
		"terminal.ansiWhite": "#ffffff",
		"terminal.ansiBrightBlack": "#636667",
		"terminal.ansiBrightRed": "#ff80ad",
		"terminal.ansiBrightGreen": "#c8ed72",
		"terminal.ansiBrightYellow": "#ebe087",
		"terminal.ansiBrightBlue": "#75e0f2",
		"terminal.ansiBrightMagenta": "#af8afe",
