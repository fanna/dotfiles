[pull]
          default = current
[push]
          default = current

[alias]
	a     = add
	all   = add -A
	st    = status -sb
	ci    = commit
	ca    = commit --amend
	br    = branch
	co    = checkout
	df    = diff
	dfc   = diff --cached
	lg    = log --pretty=format:'%C(yellow)%h%Creset %Cgreen(%><(12)%cr%><|(12))%Creset - %s %C(blue)<%an>%Creset'
	pl    = pull
	ps    = push

[color]
	diff   = auto
	status = auto
	branch = auto

[core]
	editor = vim
